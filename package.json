{
  "name": "unocss-preset-primitives",
  "type": "module",
  "version": "0.0.1-beta.3",
  "packageManager": "pnpm@7.12.0",
  "description": "A UnoCSS preset and variants for styling headlessui, radix-ui state or custom one",
  "author": "zirbest <zirbst@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/zirbest",
  "homepage": "https://github.com/zirbest/unocss-preset-primitives#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zirbest/unocss-preset-primitives.git"
  },
  "bugs": "https://github.com/zirbest/unocss-preset-primitives/issues",
  "keywords": [
    "unocss",
    "unocss-preset",
    "unocss-preset-primitives",
    "preset",
    "primitives",
    "headlessui",
    "radix-ui"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@unocss/core": "^0.45.29"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@antfu/ni": "^0.18.2",
    "@antfu/utils": "^0.5.2",
    "@types/node": "^18.11.0",
    "@vitest/ui": "^0.24.3",
    "bumpp": "^8.2.1",
    "eslint": "^8.25.0",
    "eslint-plugin-vue": "^9.6.0",
    "esno": "^0.16.3",
    "pnpm": "^7.13.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "unbuild": "^0.9.1",
    "unocss": "^0.45.29",
    "vite": "^3.1.8",
    "vitest": "^0.23.4"
  }
}
